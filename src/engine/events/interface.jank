(ns engine.events.interface
  (:refer-clojure :exclude [read])
  (:require [engine.events.core :as core]))

(defn start
  [config]
  (core/start config))

(defn stop
  [event-store]
  (core/stop event-store))

(defn read
  [event-store args]
  (core/read event-store args))

(defn append
  [event-store args]
  (core/append event-store args))
